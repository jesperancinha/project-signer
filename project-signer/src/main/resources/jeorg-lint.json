[
  {
    "find": "\n- ([a-zA-Z])",
    "replace":"\n-   $1"
  },
  {
    "find": "\n- (\\[)",
    "replace":"\n-   $1"
  },
  {
    "find": "\n([0-9])\\.([\\  ]+)(.)",
    "replace":"\n$1\\.  $3"
  },
  {
    "find": "([\\ ]+)([0-9])\\.([\\  ]+)(.)",
    "replace":"$1$2\\.  $4"
  },
  {
    "find": "\n([0-9][0-9]+)\\.([\\  ]+)(.)",
    "replace":"\n$1\\. $3"
  },
  {
    "find": "([\\ ]+)([0-9][0-9]+)\\.([\\  ]+)(.)",
    "replace": "$1$2\\. $4"
  }
]